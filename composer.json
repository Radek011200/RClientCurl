{
    "name": "radek011200/curl-client-php",
    "description": "Curl based on rest client library",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Radek011200\\CurlClientPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radek011200\\CurlClientPhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Radek011200",
            "email": "beziek120@o2.pl"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test" : "./vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "require": {
        "guzzlehttp/psr7": "2.4.x-dev",
        "php": "^8.0",
        "ext-curl": "*"
    }
}
