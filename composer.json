{
    "name": "radek011200/curl-client-php",
    "description": "Curl based on rest client library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Radek011200\\CurlClientPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radek011200\\CurlClientPhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Radek011200",
            "email": "beziek120@o2.pl"
        }
    ],
    "minimum-stability": "dev",
    "version": "1.0.3",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test" : "./vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "require": {
        "guzzlehttp/psr7": "^2.4",
        "php": "^8.1",
        "ext-curl": "*"
    }
}
